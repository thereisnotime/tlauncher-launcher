[Desktop Entry]
Version=1.0
Type=Application
Name=Minecraft Launcher Launcher
Comment=A launcher for TLauncher - containerized Minecraft launcher
Exec=python3 minecraft.py
Path=%d
Icon=minecraft-launcher
Terminal=false
Categories=Game;
StartupNotify=true

# Installation:
# 1. Edit this file and replace %d with your actual installation directory
# 2. Copy icon.png to ~/.local/share/icons/ and rename to minecraft-launcher.png
# 3. Copy this file to ~/.local/share/applications/
